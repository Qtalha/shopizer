pipeline {
    agent {label 'OPENJDK-11'}
    stages {
        stage('pull from vcs') {
        
           steps {
            git url: 'https://github.com/Qtalha/shopizer.git',
            branch: 'SPRINT_REL' 
           }
        
        }
        stage("build") {
            steps {
                
                sh 'mvn package'
            }
        }
    }
}
